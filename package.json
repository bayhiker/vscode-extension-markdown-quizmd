{
  "name": "markdown-quizmd",
  "displayName": "Markdown Preview QuizMD Support",
  "description": "Adds QuizMD authoring support to VS Code's built-in markdown preview",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.63.0"
  },
  "icon": "resources/logo.png",
  "keywords": [
    "quizmd",
    "markdown",
    "preview",
    "quiz"
  ],
  "publisher": "bayhiker",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/bayhiker/vscode-extension-markdown-quizmd.git"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:markdown-quizmd.about"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "markdown.markdownItPlugins": true,
    "commands": [
      {
        "command": "markdown-quizmd.about",
        "title": "About QuizMD"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.4",
    "@types/markdown-it-container": "^2.0.4",
    "@types/mocha": "^9.0.0",
    "@types/node": "14.x",
    "@types/vscode": "^1.63.0",
    "@typescript-eslint/eslint-plugin": "^5.1.0",
    "@typescript-eslint/parser": "^5.1.0",
    "@vscode/test-electron": "^1.6.2",
    "eslint": "^8.1.0",
    "glob": "^7.1.7",
    "mocha": "^9.1.3",
    "ts-loader": "^9.2.5",
    "typescript": "^4.4.4",
    "webpack": "^5.52.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "markdown-it-quizmd": "file:../markdown-it-quizmd"
  }
}
